{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {},
    "overrides": [
        {
            "files": [
                "*.js"
            ],
            "parserOptions": {
                "sourceType": "script"
            },
            "rules": {
                "@typescript-eslint/no-var-requires": "off"
            }
        }
    ]
}